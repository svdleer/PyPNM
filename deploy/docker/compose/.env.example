# PyPNM container image tag (for example v0.9.21.0 or latest)
PYPNM_TAG=v0.9.21.0

# Host port that maps to the container's FastAPI port
PYPNM_PORT=8000

# Optional log level hint passed to the container
PYPNM_LOG_LEVEL=info

# Optional Fernet key for encrypting config secrets (used by config-menu)
# Leave unset if you don't need encrypted passwords.
# PYPNM_SECRET_KEY=put_fernet_key_here
